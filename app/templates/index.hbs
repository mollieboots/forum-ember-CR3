<div class="body-col">
  <div class="col-md-8">
    <h1>Browse through Questions</h1>

    {{#each model.questions as |question|}}
      {{question-tile question=question favorite="favorite"}}
    {{/each}}

    <hr>

    {{new-question question=model saveQuestion="saveQuestion"}}
  </div>
  <div class="col-md-4">
    {{favorites-display}}
  </div>
</div>
